define("",["$","$s"],function(b,a){if(a.viewport().width<1204&&b("#main").width()>=650){b("#sogou_wrap_id").addClass("main-wt650")}b("#right").show();a.wait("right",function(c){var d=a.$("main");if(d){c.style.display="";if(typeof rflag!="undefined"){rflag=1}if(typeof(kmap_rsong_pos)!="undefined"){if(kmap_rsong_pos>22){a.$("kmap_song_scroll").scrollTop=kmap_rsong_pos}}}});b(window).resize(function(){var c=document.getElementById("sogou_wrap_id");a.cls(document.body,"vr1024",(a.viewport().width<1204));a.cls(c,"main-wt650",(a.viewport().width<1204))})});
